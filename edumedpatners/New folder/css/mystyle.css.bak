}
.titles{
	border-style:solid;
	border-color:#08083a;
	padding:4px;
	width:60%;
	height:1 auto;
	border-radius:4px;
	border-width:1px;
	color:#062444;
	background-color:rgba(0,0,0,0);
	text-align:center;
	transition: background-color 0.5s ease;
 
	
}
.titles:hover{
	background-color:rgba(120, 205, 120, 0.8);
}
.follow{
	border-style:solid;
	border-color:#08083a;
	padding:1.8px;
	width:40%;
	height:1 auto;
	border-radius:4px;
	border-width:1px;
	color:#062444;
	background-color:light-blue;
	text-align:center;
	transition: background-color 0.5s ease;
 
	
}
.follow:hover{
	background-color:blue;
}
.pagestrending{
	border-width:1px;
